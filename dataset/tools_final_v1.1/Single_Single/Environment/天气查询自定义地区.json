{
    "tool_name": "天气查询-自定义地区",
    "tool_description": "提供自定义地区天气信息查询的API接口",
    "home_url": "https://api.aa1.cn/doc/weatherchaxu.html",
    "country": "China",
    "api_list": [
        {
            "name": "天气查询接口",
            "url": "https://api.oioweb.cn/api/weather/weather",
            "description": "查询指定城市的天气信息",
            "method": "GET",
            "required_parameters": [
                {
                    "name": "city_name",
                    "type": "string",
                    "description": "指定查询的城市名称",
                    "default": "郑州市"
                }
            ],
            "optional_parameters": [
                {
                    "name": "language",
                    "type": "string",
                    "description": "返回的语言类型，如中文、英文等",
                    "default": "zh"
                },
                {
                    "name": "unit",
                    "type": "string",
                    "description": "温度单位，如摄氏度（c）、华氏度（f）",
                    "default": "c"
                }
            ],
            "statuscode": "200",
            "schema": {}
        }
    ]
}