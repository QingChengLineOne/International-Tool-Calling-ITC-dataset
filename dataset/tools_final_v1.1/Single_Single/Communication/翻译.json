{
    "tool_name": "翻译API",
    "tool_description": "一个简单的翻译接口，支持多种语言之间的翻译",
    "home_url": "https://api.aa1.cn/doc/52fanyi.html",
    "country": "China",
    "api_list": [
        {
            "name": "翻译接口",
            "url": "https://api.52vmy.cn/api/query/fanyi",
            "description": "执行翻译操作",
            "method": "GET",
            "required_parameters": [
                {
                    "name": "msg",
                    "type": "string",
                    "description": "需要翻译的文本内容",
                    "default": "hello"
                }
            ],
            "optional_parameters": [
                {
                    "name": "from_lang",
                    "type": "string",
                    "description": "源语言代码，默认为auto，自动检测",
                    "default": "auto"
                },
                {
                    "name": "to_lang",
                    "type": "string",
                    "description": "目标语言代码，例如en表示英语，zh表示中文等",
                    "default": "zh"
                }
            ],
            "statuscode": "200",
            "schema": {}
        }
    ]
}