            "method": "GET",
            "required_parameters": [
                {
                    "name": "key",
                    "type": "string",
                    "description": "api_key",
                    "default": "6LDBZ-BF4HK-G2ZJA-ABCNH-3A44S-7OFZI"
                },
                {
                    "name": "mode",
                    "type": "string",
                    "description": "计算方式：driving（驾车）、walking（步行）。默认：driving",
