                    "default": ""
                }
            ],
            "optional_parameters": [
                {
                    "name": "output",
                    "type": "string",
                    "description": "返回格式",
                    "default": ""
                },
                {
                    "name": "callback",
                    "type": "string",
                    "description": "JSONP方式回调函数",
                    "default": ""
                }
            ],
            "statuscode": "200",
            "schema": {}
        }
    ]
