{
    "tool_name": "历史上的今天API",
    "tool_description": "提供历史上的今天相关事件、名人等的查询服务",
    "home_url": "https://api.aa1.cn/doc/history-xx.html",
    "country": "China",
    "api_list": [
        {
            "name": "历史上的今天查询接口",
            "url": "https://v2.api-m.com/api/history",
            "description": "获取指定日期历史上的事件、名人等信息",
            "method": "GET",
            "required_parameters": [
                {
                    "name": "date",
                    "type": "string",
                    "description": "查询的日期，格式为YYYY-MM-DD",
                    "default": "今天"
                }
            ],
            "optional_parameters": [
                {
                    "name": "type",
                    "type": "string",
                    "description": "事件类型，可选值为event（事件）、person（名人）、birth（出生）、death（逝世）",
                    "default": "event"
                },
                {
                    "name": "limit",
                    "type": "int",
                    "description": "返回结果数量限制，默认为10",
                    "default": 10
                }
            ],
            "statuscode": "200",
            "schema": {}
        }
    ]
}