{
    "tool_name": "抖音视频搜索",
    "tool_description": "搜索抖音的视频列表",
    "home_url": "https://api.aa1.cn/doc/douyinsearchvideo.html",
    "country": "China",
    "api_list": [
        {
            "name": "抖音视频搜索API",
            "url": "https://api.pearktrue.cn/api/dy/search/",
            "description": "通过关键字搜索抖音视频列表",
            "method": "GET",
            "required_parameters": [
                {
                    "name": "keyword",
                    "type": "string",
                    "description": "搜索视频的关键字",
                    "default": ""
                },
                {
                    "name": "page",
                    "type": "int",
                    "description": "搜索结果的页码，从1开始",
                    "default": 1
                }
            ],
            "optional_parameters": [
                {
                    "name": "count",
                    "type": "int",
                    "description": "每页返回的视频数量，默认为20",
                    "default": 20
                },
                {
                    "name": "sort",
                    "type": "string",
                    "description": "排序方式，可选值为time(时间排序)、hot(热度排序)",
                    "default": "hot"
                },
                {
                    "name": "filter",
                    "type": "string",
                    "description": "筛选条件，可选值为douyin(抖音视频)、all(所有视频)",
                    "default": "douyin"
                }
            ],
            "statuscode": "200",
            "schema": {}
        }
    ]
}