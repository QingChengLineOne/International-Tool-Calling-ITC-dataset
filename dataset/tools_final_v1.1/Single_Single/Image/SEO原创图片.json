            "method": "GET",
            "required_parameters": [
                {
                    "name": "wd",
                    "type": "string",
                    "description": "需要生成图片的文案",
                    "default": ""
                }
            ],
            "optional_parameters": [
                {
                    "name": "size",
                    "type": "string",
                    "description": "图片大小，如'500x500'，默认为'500x500'",
