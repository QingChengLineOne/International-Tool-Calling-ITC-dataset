                },
                {
                    "name": "font_size",
                    "type": "int",
                    "description": "文字大小，单位为像素，默认为'24'",
                    "default": "24"
                },
                {
                    "name": "color",
                    "type": "string",
                    "description": "文字颜色，如'#FFFFFF'，默认为'#000000'",
                    "default": "#000000"
                },
                {
                    "name": "background",
                    "type": "string",
                    "description": "背景颜色，如'#000000'，默认为'#FFFFFF'",
                    "default": "#FFFFFF"
                }
            ],
            "statuscode": "200",
            "schema": {}
        }
    ]
